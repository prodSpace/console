{
    "name": "prodspace/console",
    "description": "A collection of commandline commands to unify that usage.",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "symfony/console": "^5.2",
        "symfony/http-client": "^5.2",
        "symfony/yaml": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "ProdSpace\\Console\\": "src/"
        }
    },
    "extra": {
